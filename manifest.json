{
  "name": "Prep.Me",
  "version": "0.1",
  "permissions": ["declarativeContent", "storage"],
  "background": {
      "scripts": ["background.js"],
      "persistent": false
  },
  "browser_action": {
    "default_popup": "popup.html"
  },
  "options_page": "options.html",
  "options_ui": {
    "page": "options.html",
    "chrome_style": true
  },

  "manifest_version": 2
}
